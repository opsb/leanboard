{
  "manifest_version": 2,
  "name": "Leanboard",
  "description": "Leanboard trello extension",
  "version": "0.1",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
